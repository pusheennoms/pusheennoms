{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": [
      "./controllers/",
      "./public/",
      "./middlewares/"
    ],
    "exclude": [
      "./public/css/",
      "./public/imgs/"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": ""
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": true,
    "destination": "./out/",
    "encoding": "utf8"
  }
}
